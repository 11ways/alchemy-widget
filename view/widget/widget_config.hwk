<% makeDialog() %>
<% addClass('widget-config-dialog') %>

<div class="widget-config-wrapper js-he-ready-wcd">
	<al-form
		#document={% widget_settings %}
		#schema={% schema %}
		class="main-form"
	>
		{% each fields as field %}
			<al-field
				field-name={% field.name %}
			></al-field>
		{% /each %}
	</al-form>

	<button class="btn btn-apply">
		Apply
	</button>
</div>