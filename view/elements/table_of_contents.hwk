{% macro printEntries %}
	<ol>
		{% each entries as entry %}
			{% run printEntry entry=entry %}
			
			{% if entry.children %}
				<li>
					{% run printEntries entries=entry.children %}
				</li>
			{% /if %}
		{% /each %}
	</ol>
{% /macro %}

{% macro printEntry %}
	<li>
		<a href="#{% entry.id %}">
			{{ entry.title }}
		</a>
	</li>
{% /macro %}

{% run printEntries entries=self.entries}